{
    "name": "r15ch13/peculiar",
    "description": "An extension to the Laravel model. It allows the use of UUIDs as primary keys.",
    "type": "library",
    "keywords": ["laravel", "model", "uuid", "guid", "ardent", "sentry"],
    "homepage": "https://github.com/r15ch13/peculiar",
    "license": "MIT",
    "authors": [
        {
            "name": "r15ch13",
            "email": "r15ch13@gmail.com",
            "homepage": "http://r15ch13.de"
        }
    ],
    "support": {
        "issues": "https://github.com/r15ch13/peculiar/issues",
        "source": "https://github.com/r15ch13/peculiar"
    },
    "require": {
        "illuminate/database": "~4.0|~5.0",
        "ramsey/uuid": "~2.8"
    },
    "require-dev": {
        "cartalyst/sentry": "~2.1",
        "laravelbook/ardent": "~2.0",
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Rfifteen\\Peculiar\\": "src/"
        }
    },
    "suggest": {
        "cartalyst/sentry": "Extends the models of Cartalyst Sentry.",
        "laravelbook/ardent": "Extends the model of Laravelbook Ardent."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "stable"
}
