{
    "name": "r15ch13/peculiar",
    "description": "An extension to the Laravel Eloquent Model. It allows the use of UUIDs (ramsey/uuid) as primary keys.",
    "type": "library",
    "keywords": ["laravel", "model", "uuid", "guid", "ardent", "sentry", "eloquent"],
    "homepage": "https://github.com/r15ch13/peculiar",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Kuhnt",
            "email": "r15ch13+git@gmail.com",
            "homepage": "https://r15ch13.de"
        }
    ],
    "support": {
        "issues": "https://github.com/r15ch13/peculiar/issues",
        "source": "https://github.com/r15ch13/peculiar"
    },
    "require": {
        "illuminate/database": "~4.0|~5.0",
        "ramsey/uuid": "~3.6"
    },
    "require-dev": {
        "cartalyst/sentry": "~2.1",
        "laravelbook/ardent": "~2.0",
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Rfifteen\\Peculiar\\": "src/"
        }
    },
    "suggest": {
        "cartalyst/sentry": "Extends the models of Cartalyst Sentry.",
        "laravelbook/ardent": "Extends the model of Laravelbook Ardent."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "stable"
}
